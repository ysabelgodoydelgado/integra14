<odoo>
    <data>
        <record id='library_rent_wizard_form' model='ir.ui.view'>
            <field name='name'>Generar Comprobante</field>
            <field name='model'>txt.wizard</field>
            <field name='arch' type='xml'>
                <form string="Generar txt">
                    <sheet>
                        <group>
                            <field name='date_start'/>
                        </group>
                        <group>
                            <field name='date_end'/>
                        </group>
                    </sheet>
                    <footer>
                        <button string='Generar txt' type='object' name='generte_txt' class='btn-primary'/>
                        <button string='Cancel' class='btn-default' special='cancel'/>
                    </footer>
                </form>
            </field>
        </record>

        <record id="action_generate_txt" model="ir.actions.act_window">
            <field name="name">Generar txt</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">txt.wizard</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>

        <menuitem action="action_generate_txt" id="txt_report"
                  parent="binaural_facturacion_reportes.account_report_menu" groups="account.group_account_manager"/>
    </data>
</odoo>